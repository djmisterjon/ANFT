    "configurations": [
        {
            "type": "nwjs",
            "request": "launch",
            "name": "Launch NWjs",
            "runtimeExecutable": "${workspaceRoot}\\_NW\\sdk\\nw.exe",
            "runtimeArgs": [
                "--disable-raf-throttling",
                "--remote-debugging-port=9222",
            ],
            "webRoot": "${workspaceRoot}",
            "port": 9222,
            "reloadAfterAttached": true,

        },
    ]
}